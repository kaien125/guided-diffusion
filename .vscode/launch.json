                "True",
                "--noise_schedule",
                "cosine",
                "--num_channels",
                "192",
                "--num_head_channels",
                "64",
                "--num_res_blocks",
                "3",
                "--resblock_updown",
                "True",
                "--use_new_attention_order",
                "True",
                "--use_fp16",
                "True",
                "--use_scale_shift_norm",
                "True",
                "--classifier_scale",
                "1.0",
                "--classifier_path",
                "./models/64x64_classifier.pt",
                "--classifier_depth",
                "4",
                "--model_path",
                "./models/64x64_diffusion.pt",
                "--batch_size",
                "2",
                "--num_samples",
                "1",
                "--timestep_respacing",
